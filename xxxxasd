[32m+[m[32mconst sections = document.querySelectorAll('.section');[m
[32m+[m[32mconst sectBtns = document.querySelectorAll('.controlls');[m
[32m+[m[32mconst sectBtn = document.querySelectorAll('.control');[m
[32m+[m[32mconst allSections= document.querySelector('.main-content');[m
[32m+[m
[32m+[m
[32m+[m[32mfunction PageTransitions() {[m
[32m+[m[32m    //Button click active class[m
[32m+[m[32m    for(let i = 0; i < sectBtn.length; i++) {[m
[32m+[m[32m        sectBtn[i].addEventListener('click', function(){[m
[32m+[m[32m            let currentBtn = document.querySelectorAll('.active-btn');[m
[32m+[m[32m            currentBtn[0].className = currentBtn[0].className.replace('active-btn', '');[m
[32m+[m[32m            this.className += ' active-btn';[m
[32m+[m[32m        })[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    //Sections Active class[m
[32m+[m[32m    allSections.addEventListener('click', (e) =>{[m
[32m+[m[32m        const id = e.target.dataset.id;[m
[32m+[m[32m        if(id){[m
[32m+[m[32m            //remove selected from the other btns[m
[32m+[m[32m            const btns = sectBtns[0].children;[m
[32m+[m[32m            for (let btn of btns){[m
[32m+[m[32m                btn.classList.remove('active')[m
[32m+[m[32m            }[m
[32m+[m[32m            e.target.classList.add('active')[m
[32m+[m
[32m+[m[32m            //hide other sections[m
[32m+[m[32m            sections.forEach((section)=>{[m
[32m+[m[32m                section.classList.remove('active')[m
[32m+[m[32m            })[m
[32m+[m[32m            const element = document.getElementById(id)[m
[32m+[m[32m            element.classList.add('active');[m
[32m+[m[32m        }[m
[32m+[m[32m    })[m
[32m+[m
[32m+[m[32m    //Toggle theme[m
[32m+[m[32m    const ThemeBtn = document.querySelector('.theme-btn');[m
[32m+[m[32m    ThemeBtn.addEventListener('click', () => {[m
[32m+[m[32m        let element = document.body;[m
[32m+[m[32m        element.classList.toggle('light-mode')[m
[32m+[m[32m    })[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32mPageTransitions();[m
